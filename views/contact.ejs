<%- include("partials/header"); -%>
    <h1>Contact</h1>
    <p> <%= contactContent %> </p>
    <h1>Feedback</h1>
    <form class="" action="/compose" method="post">
      <div class="form-group">
        <label>EMAIL</label>
        <input class="form-control" type="text" name="postTitle">
        <label>Comment</label>
        <textarea class="form-control" name="postBody" rows="5" cols="30"></textarea>
      </div>
      <button class="btn btn-primary" type="submit" name="button">Send</button>
    </form>
<%- include("partials/footer"); -%>
